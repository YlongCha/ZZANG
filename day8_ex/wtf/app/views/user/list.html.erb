<% @posts.each do |post| %>
<p><%= post.id %></p>
<p><%= post.username %></p>
<p><%= post.title %></p>
<p><%= post.content %></p>
<p><%= post.created_at %></p>
<a href="/user/show/<%=post.id%>" class="btn btn-info" role="button" aria-pressed="true">보기</a>
<a href="/user/edit/<%=post.id%>" class="btn btn-info" role="button" aria-pressed="true">수정</a>
<a href="/user/destroy/<%=post.id%>" class="btn btn-dark" role="button" aria-pressed="true">삭제</a>
<% end%>
